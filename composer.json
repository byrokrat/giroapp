        "byrokrat/banking": "^2.1",
        "byrokrat/id": "^2",
        "composer/semver": "^1",
        "hanneskod/yaysondb": "^2.2",
        "hanneskod/clean": "^2",
        "hashids/hashids": "^4",
        "league/tactician": "^1",
        "moneyphp/money": "^3",
        "psr/event-dispatcher": "^1",
        "fig/event-dispatcher-util": "^1",
        "crell/tukio": "^1",
        "psr/log": "^1",
        "katzgrau/klogger": "^1",
        "symfony/dependency-injection": "^4",
        "symfony/console": "^4",
        "symfony/finder": "^4",
        "symfony/filesystem": "^4",
        "symfony/proxy-manager-bridge": "^4",
        "symfony/workflow": "^5.0@dev",
        "fzaninotto/streamer": "0.*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3",
        "symfony/config": "^4",
        "symfony/yaml": "^4",
        "symfony/expression-language": "^4"
    }
