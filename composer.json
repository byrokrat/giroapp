{
    "name": "byrokrat/giroapp",
    "description": "Command line app for managing autogiro donations",
    "keywords": ["autogiro"],
    "homepage": "https://github.com/byrokrat/giroapp",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Hannes ForsgÃ¥rd",
            "email": "hannes.forsgard@fripost.org"
        }
    ],
    "bin": [
        "bin/giroapp"
    ],
    "autoload": {
        "psr-4": {
            "byrokrat\\giroapp\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\byrokrat\\giroapp\\": ["spec/"]
